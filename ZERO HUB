-- Buscador RÃ¡pido
local SearchTab = Window:CreateTab("ğŸ” Buscador Teleports", 4483362458)
local SearchInput = SearchTab:CreateInput({
   Name = "Busca Isla (ej: Green, Hydra)",
   PlaceholderText = "Green Zone",
   Callback = function(Text)
      if Text:lower():find("marine") then TeleportTo(CFrame.new(36, 7, 16)) end
      elseif Text:lower():find("jungle") then TeleportTo(CFrame.new(-1604, 36, 142)) end
      elseif Text:lower():find("green") then TeleportTo(CFrame.new(-2888, 43, 5580)) end
      elseif Text:lower():find("hydra") then TeleportTo(CFrame.new(-5566, 312, 492)) end
      -- AÃ±ade mÃ¡s...
   end
})

