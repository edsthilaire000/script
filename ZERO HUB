    if S.FN or S.FQ then for _,v in pairs(WS.Enemies:GetChildren())do if v:FindFirstChild("Humanoid")and v.Humanoid.Health>0 then r.CFrame=v.HumanoidRootPart.CFrame*CFrame.new(0,10,0);v.Humanoid.Health=0;RS.Remotes.CommF_:InvokeServer("StartQuest")end end end
    if S.FB then for _,v in pairs(WS.Bosses:GetChildren())do if v:FindFirstChild("HumanoidRootPart")then r.CFrame=v.HumanoidRootPart.CFrame*CFrame.new(0,10,0);v.Humanoid.Health=0 end end end
    if S.FM then RS.Remotes.CommF_:InvokeServer("AddPoint",S.AS,1)end
    if S.FR then RS.Remotes.CommF_:InvokeServer("Raid","Flame")end
end end)
spawn(function()while wait(1)do 
    if S.FS then for _,v in pairs(WS:GetChildren())do local n=v.Name:lower()if n:find("mammoth")or n:find("dragon")or n:find("leopard")or n:find("kitsune")then firetouchinterest(r,v.Handle,0);firetouchinterest(r,v.Handle,1)end end end
    if S.FSea then RS.Remotes.CommF_:InvokeServer("SeaBeast")end
    if S.FV4 then RS.Remotes.CommF_:InvokeServer("RaceV4")end
    if S.FVol then r.CFrame=TPs.Castle end
    if S.FCDK then RS.Remotes.CommF_:InvokeServer("CursedDualKatana")end
