-- Función para crear botones estilo Redz
local function CreateButton(parent, text, position, callback)
    local Button = Instance.new("TextButton")
    local UICorner = Instance.new("UICorner")
    
    Button.Name = text.."Button"
    Button.Parent = parent
    Button.BackgroundColor3 = Color3.fromRGB(45, 45, 55)
    Button.Position = position
    Button.Size = UDim2.new(0, 180, 0, 35)
    Button.Font = Enum.Font.GothamSemibold
    Button.Text = text
    Button.TextColor3 = Color3.fromRGB(255, 255, 255)
    Button.TextSize = 14.000
    Button.AutoButtonColor = false
    
    UICorner.CornerRadius = UDim.new(0, 6)
    UICorner.Parent = Button
    
    -- Efectos del botón
    Button.MouseEnter:Connect(function()
        TweenService:Create(Button, TweenInfo.new(0.2), {
            BackgroundColor3 = Color3.fromRGB(55, 55, 65)
        }):Play()
    end)
    
    Button.MouseLeave:Connect(function()
        TweenService:Create(Button, TweenInfo.new(0.2), {
            BackgroundColor3 = Color3.fromRGB(45, 45, 55)
        }):Play()
    end)
    
    Button.MouseButton1Click:Connect(callback)
    
    return Button
end

-- Función para crear toggles estilo Redz
local function CreateToggle(parent, text, position, callback)
    local ToggleFrame = Instance.new("Frame")
    local UICorner = Instance.new("UICorner")
    local ToggleStatus = Instance.new("TextLabel")
    local ToggleIndicator = Instance.new("Frame")
    local UICorner_3 = Instance.new("UICorner")
    
    ToggleFrame.Name = text.."Toggle"
    ToggleFrame.Parent = parent
    ToggleFrame.BackgroundColor3 = Color3.fromRGB(35, 35, 45)
    ToggleFrame.Position = position
    ToggleFrame.Size = UDim2.new(0, 180, 0, 40)
    
    UICorner.CornerRadius = UDim.new(0, 6)
    UICorner.Parent = ToggleFrame
    
    ToggleStatus.Name = "Status"
    ToggleStatus.Parent = ToggleFrame
    ToggleStatus.BackgroundTransparency = 1
    ToggleStatus.Position = UDim2.new(0, 15, 0, 0)
    ToggleStatus.Size = UDim2.new(1, -65, 1, 0)
    ToggleStatus.Font = Enum.Font.GothamSemibold
    ToggleStatus.Text = text
    ToggleStatus.TextColor3 = Color3.fromRGB(255, 255, 255)
    ToggleStatus.TextSize = 14.000
    ToggleStatus.TextXAlignment = Enum.TextXAlignment.Left
    
    ToggleIndicator.Name = "Indicator"
    ToggleIndicator.Parent = ToggleFrame
    ToggleIndicator.BackgroundColor3 = Color3.fromRGB(255, 80, 80)
    ToggleIndicator.Position = UDim2.new(1, -50, 0.5, -10)
    ToggleIndicator.Size = UDim2.new(0, 35, 0, 20)
    
    UICorner_3.CornerRadius = UDim.new(0, 10)
    UICorner_3.Parent = ToggleIndicator
    
    local isToggled = false
    
    local function UpdateToggle()
        if isToggled then
            TweenService:Create(ToggleIndicator, TweenInfo.new(0.2), {
                BackgroundColor3 = Color3.fromRGB(80, 255, 80),
                Position = UDim2.new(1, -25, 0.5, -10)
            }):Play()
        else
            TweenService:Create(ToggleIndicator, TweenInfo.new(0.2), {
                BackgroundColor3 = Color3.fromRGB(255, 80, 80),
                Position = UDim2.new(1, -50, 0.5, -10)
            }):Play()
        end
        callback(isToggled)
    end
    
    ToggleFrame.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            isToggled = not isToggled
            UpdateToggle()
        end
    end)
    
    return ToggleFrame
end

-- Crear tabs estilo Redz
local function CreateTab(text)
    local TabButton = Instance.new("TextButton")
    local UICorner = Instance.new("UICorner")
    
    TabButton.Name = text.."Tab"
    TabButton.Parent = TabsFrame
    TabButton.BackgroundColor3 = Color3.fromRGB(35, 35, 45)
    TabButton.Size = UDim2.new(1, -20, 0, 35)
    TabButton.Position = UDim2.new(0, 10, 0, (#TabsFrame:GetChildren() - 1) * 40)
    TabButton.Font = Enum.Font.GothamSemibold
    TabButton.Text = text
    TabButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    TabButton.TextSize = 14.000
    TabButton.AutoButtonColor = false
    
    UICorner.CornerRadius = UDim.new(0, 6)
    UICorner.Parent = TabButton
    
    local ContentPage = Instance.new("ScrollingFrame")
    ContentPage.Name = text.."Page"
    ContentPage.Parent = ContentFrame
    ContentPage.BackgroundTransparency = 1
    ContentPage.Position = UDim2.new(0, 10, 0, 10)
    ContentPage.Size = UDim2.new(1, -20, 1, -20)
    ContentPage.ScrollBarThickness = 3
    ContentPage.CanvasSize = UDim2.new(0, 0, 0, 500)
    ContentPage.Visible = false
    
    return TabButton, ContentPage
end

-- Crear los tabs principales
local FarmingTab, FarmingPage = CreateTab("⚡ Farming")
local TeleportsTab, TeleportsPage = CreateTab("📍 Teleports")
local PlayerTab, PlayerPage = CreateTab("👤 Player")
local MiscTab, MiscPage = CreateTab("🔧 Misc")

-- Mostrar la página de Farming por defecto
FarmingPage.Visible = true
FarmingTab.BackgroundColor3 = Color3.fromRGB(45, 45, 55)

-- Sistema de cambio de tabs
local function SwitchTab(tabButton, page)
    tabButton.MouseButton1Click:Connect(function()
        for _, v in pairs(ContentFrame:GetChildren()) do
            if v:IsA("ScrollingFrame") then
                v.Visible = false
            end
        end
        page.Visible = true
        
        for _, v in pairs(TabsFrame:GetChildren()) do
            if v:IsA("TextButton") then
                TweenService:Create(v, TweenInfo.new(0.2), {
                    BackgroundColor3 = Color3.fromRGB(35, 35, 45)
                }):Play()
            end
        end
        TweenService:Create(tabButton, TweenInfo.new(0.2), {
            BackgroundColor3 = Color3.fromRGB(45, 45, 55)
        }):Play()
    end)
end

SwitchTab(FarmingTab, FarmingPage)
SwitchTab(TeleportsTab, TeleportsPage)
SwitchTab(PlayerTab, PlayerPage)
SwitchTab(MiscTab, MiscPage)

-- ========== SISTEMA DE FARMING ==========
CreateToggle(FarmingPage, "Auto Farm", UDim2.new(0, 10, 0, 10), function(state)
    getgenv().AutoFarmEnabled = state
    if state then
        spawn(function()
            while getgenv().AutoFarmEnabled and wait(0.5) do
                pcall(function()
                    local enemies = workspace:FindFirstChild("Enemies")
                    if enemies then
                        for _, enemy in pairs(enemies:GetChildren()) do
                            if getgenv().AutoFarmEnabled and enemy:FindFirstChild("Humanoid") and enemy.Humanoid.Health > 0 then
                                Player.Character:SetPrimaryPartCFrame(enemy.HumanoidRootPart.CFrame * CFrame.new(0, 0, -8))
                                wait(0.3)
                                for _, tool in pairs(Player.Character:GetChildren()) do
                                    if tool:IsA("Tool") then
                                        tool:Activate()
                                    end
                                end
                            end
                        end
                    end
                end)
            end
        end)
    end
end)

CreateToggle(FarmingPage, "Auto Buso Haki", UDim2.new(0, 10, 0, 60), function(state)
    getgenv().AutoBusoEnabled = state
    if state then
        spawn(function()
            while getgenv().AutoBusoEnabled and wait(10) do
                pcall(function()
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Buso")
                end)
            end
        end)
    end
end)

CreateToggle(FarmingPage, "Auto Click", UDim2.new(0, 10, 0, 110), function(state)
    getgenv().AutoClickEnabled = state
    if state then
        spawn(function()
            while getgenv().AutoClickEnabled and wait(0.1) do
                pcall(function()
                    mouse1click()
                end)
            end
        end)
    end
end)

-- ========== SISTEMA DE TELEPORTS ==========
local Islands = {
    {"🏴‍☠️ Pirate Starter", CFrame.new(1099, 49, 1555)},
    {"⚓ Marine Starter", CFrame.new(-2828, 49, 2093)},
    {"🌴 Jungle", CFrame.new(-1500, 70, -200)},
    {"🏠 Pirate Village", CFrame.new(-1150, 50, 3850)},
    {"🏜️ Desert", CFrame.new(950, 50, 4100)},
    {"❄️ Frozen Village", CFrame.new(1150, 150, -1250)},
    {"🏰 Marine Fortress", CFrame.new(-4850, 331, -360)},
    {"☁️ Skylands", CFrame.new(-500, 450, -200)},
    {"🔒 Prison", CFrame.new(4900, 50, 500)}
}

for i, island in pairs(Islands) do
    CreateButton(TeleportsPage, island[1], UDim2.new(0, 10, 0, 10 + ((i-1) * 40)), function()
        pcall(function()
            Player.Character:SetPrimaryPartCFrame(island[2])
            CreateNotification("TELEPORT", "Viajando a: " .. island[1], 3)
        end)
    end)
end

-- ========== MODIFICACIONES DE PLAYER ==========
CreateButton(PlayerPage, "🚀 Walkspeed +50", UDim2.new(0, 10, 0, 10), function()
    pcall(function()
        Player.Character.Humanoid.WalkSpeed = 50
        CreateNotification("PLAYER", "Walkspeed aumentado a 50", 3)
    end)
end)

CreateButton(PlayerPage, "🦘 JumpPower +100", UDim2.new(0, 10, 0, 60), function()
    pcall(function()
        Player.Character.Humanoid.JumpPower = 100
        CreateNotification("PLAYER", "JumpPower aumentado a 100", 3)
    end)
end)

CreateToggle(PlayerPage, "Noclip", UDim2.new(0, 10, 0, 110), function(state)
    getgenv().NoclipEnabled = state
    if state then
        spawn(function()
            while getgenv().NoclipEnabled and wait() do
                pcall(function()
                    for _, part in pairs(Player.Character:GetDescendants()) do
                        if part:IsA("BasePart") then
                            part.CanCollide = false
                        end
                    end
                end)
            end
        end)
    end
end)

-- ========== MISCELÁNEOS ==========
CreateButton(MiscPage, "🔄 Rejoin Server", UDim2.new(0, 10, 0, 10), function()
    TeleportService:Teleport(game.PlaceId, Player)
end)

CreateButton(MiscPage, "🌐 Server Hop", UDim2.new(0, 10, 0, 60), function()
    TeleportService:TeleportToPlaceInstance(game.PlaceId, game.JobId)
end)

CreateButton(MiscPage, "⏰ Anti AFK", UDim2.new(0, 10, 0, 110), function()
    local VirtualUser = game:GetService("VirtualUser")
    Player.Idled:connect(function()
        VirtualUser:CaptureController()
        VirtualUser:ClickButton2(Vector2.new())
    end)
    CreateNotification("MISC", "Anti AFK Activado", 3)
end)

-- ========== NOTIFICACIONES ==========
function CreateNotification(title, text, duration)
    local NotificationFrame = Instance.new("Frame")
    local UICorner = Instance.new("UICorner")
    local TitleLabel = Instance.new("TextLabel")
    local MessageLabel = Instance.new("TextLabel")
    
    NotificationFrame.Name = "Notification"
    NotificationFrame.Parent = ScreenGui
    NotificationFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
    NotificationFrame.Position = UDim2.new(1, -310, 1, -100)
    NotificationFrame.Size = UDim2.new(0, 300, 0, 80)
    
    UICorner.CornerRadius = UDim.new(0, 8)
    UICorner.Parent = NotificationFrame
    
    TitleLabel.Name = "Title"
    TitleLabel.Parent = NotificationFrame
    TitleLabel.BackgroundTransparency = 1
    TitleLabel.Position = UDim2.new(0, 15, 0, 10)
    TitleLabel.Size = UDim2.new(1, -30, 0, 20)
    TitleLabel.Font = Enum.Font.GothamBold
    TitleLabel.Text = title
    TitleLabel.TextColor3 = Color3.fromRGB(0, 255, 255)
    TitleLabel.TextSize = 16.000
    TitleLabel.TextXAlignment = Enum.TextXAlignment.Left
    
    MessageLabel.Name = "Message"
    MessageLabel.Parent = NotificationFrame
    MessageLabel.BackgroundTransparency = 1
    MessageLabel.Position = UDim2.new(0, 15, 0, 35)
    MessageLabel.Size = UDim2.new(1, -30, 0, 35)
    MessageLabel.Font = Enum.Font.Gotham
    MessageLabel.Text = text
    MessageLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
    MessageLabel.TextSize = 14.000
    MessageLabel.TextXAlignment = Enum.TextXAlignment.Left
    MessageLabel.TextWrapped = true
    
    -- Animación de entrada
    NotificationFrame.Position = UDim2.new(1, 0, 1, -100)
    TweenService:Create(NotificationFrame, TweenInfo.new(0.5, Enum.EasingStyle.Quart), {
        Position = UDim2.new(1, -310, 1, -100)
    }):Play()
    
    -- Animación de salida
    spawn(function()
        wait(duration)
        TweenService:Create(NotificationFrame, TweenInfo.new(0.5, Enum.EasingStyle.Quart), {
            Position = UDim2.new(1, 0, 1, -100)
        }):Play()
        wait(0.5)
        NotificationFrame:Destroy()
    end)
end

-- ========== INICIALIZACIÓN ==========
-- Tecla para mostrar/ocultar el menú
UserInputService.InputBegan:Connect(function(input)
    if input.KeyCode == Enum.KeyCode.RightControl then
        MainFrame.Visible = not MainFrame.Visible
    end
end)

-- Anti AFK automático
local VirtualUser = game:GetService("VirtualUser")
Player.Idled:connect(function()
    VirtualUser:CaptureController()
    VirtualUser:ClickButton2(Vector2.new())
end)

-- Notificación de inicio
CreateNotification("ZEIRO HUB", "Script cargado correctamente!\nPresiona RightControl para abrir/cerrar", 5)

print("🎯 ZEIRO HUB PREMIUM CARGADO!")
print("⚡ Sistema de Farming: Auto Farm, Auto Buso, Auto Click")
print("📍 Sistema de Teleports: " .. #Islands .. " islas disponibles")
print("👤 Modificaciones de Player: Walkspeed, JumpPower, Noclip")
print("🔧 Utilidades: Rejoin, Server Hop, Anti AFK")
