-- ZERO HUB PRO v4.1 | 50+ TELEPORTS ALL SEAS | Multi-Hub + Anti-Lag | Gravity 2025
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local TweenService = game:GetService("TweenService")
local Players = game:GetService("Players")
local player = Players.LocalPlayer

local Window = Rayfield:CreateWindow({
   Name = "ğŸŸ¢ ZERO HUB PRO v4.1 [50+ TELEPORTS]",
   LoadingTitle = "Cargando 50+ Teleports...",
   LoadingSubtitle = "Sea 1-4 + Multi-Hub | Nov 2025",
   ConfigurationSaving = { Enabled = true, FolderName = "ZeroHubV4.1", FileName = "Config" },
   KeySystem = false
})

-- TWEEN TP (Anti-Kick)
local function TeleportTo(cf)
   local char = player.Character
   if char and char:FindFirstChild("HumanoidRootPart") then
      local tween = TweenService:Create(char.HumanoidRootPart, TweenInfo.new(0.8, Enum.EasingStyle.Quad), {CFrame = cf * CFrame.new(0, 5, 0)})
      tween:Play()
   end
end

-- Multi-Hub Loader (Igual v4.0)
local MainTab = Window:CreateTab("ğŸš€ Multi-Hub Loader", 4483362458)
MainTab:CreateSection("Carga Hubs PRO")
local SelectedHub = "REDz Hub"
MainTab:CreateDropdown({
   Name = "Seleccionar Hub", Options = {"REDz Hub", "Xero Hub", "Quantum Onyx", "Ro-Hub", "Zen Hub", "Blue X Hub"},
   CurrentOption = "REDz Hub", Callback = function(Option) SelectedHub = Option end
})
MainTab:CreateButton({
   Name = "ğŸ”¥ Load Hub (Auto Farm/Boss)",
   Callback = function()
      local hubCode
      if SelectedHub == "REDz Hub" then hubCode = 'local Settings = { JoinTeam = "Pirates", Translator = true }; getgenv().BETA_VERSION = true; loadstring(game:HttpGet("https://raw.githubusercontent.com/tlredz/Scripts/refs/heads/main/main.luau"))(Settings)'
      elseif SelectedHub == "Xero Hub" then hubCode = 'getgenv().Team = "Pirates"; loadstring(game:HttpGet("https://raw.githubusercontent.com/Xero2409/XeroHub/refs/heads/main/main.lua"))()'
      elseif SelectedHub == "Quantum Onyx" then hubCode = 'loadstring(game:HttpGet("https://raw.githubusercontent.com/flazhy/QuantumOnyx/refs/heads/main/QuantumOnyx.lua"))()'
      elseif SelectedHub == "Ro-Hub" then hubCode = 'loadstring(game:HttpGet("https://raw.githubusercontent.com/RO-HUB-CODEX/RO-HUB/refs/heads/main/bloxfruits.lua"))()'
      elseif SelectedHub == "Zen Hub" then hubCode = 'loadstring(game:HttpGet("https://raw.githubusercontent.com/Zenhubtop/zen_hub_pr/main/zennewwwwui.lua", true))()'
      else hubCode = 'loadstring(game:HttpGet("https://raw.githubusercontent.com/Dev-BlueX/BlueX-Hub/refs/heads/main/Main.lua"))()' end
      loadstring(hubCode)()
      Rayfield:Notify({Title = SelectedHub.." Loaded!", Content = "Â¡Auto Farm Activado!", Duration = 4})
   end
})

